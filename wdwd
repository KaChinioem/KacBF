do
    local ui = game.CoreGui:FindFirstChild("					")
    if ui then
        ui:Destroy()
     end
end

local kachini = Instance.new("ScreenGui")
local kachini1 = Instance.new("Frame")
local kachini2 = Instance.new("UICorner")
local kachini3 = Instance.new("TextLabel")

kachini.Name = "isEngineTruncationEnabledForIngameSettings"
kachini.Parent = game.CoreGui
kachini.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

kachini1.Name = "FFlagVRLetRaycastsThroughUI"
kachini1.Parent = kachini
kachini1.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
kachini1.BorderColor3 = Color3.fromRGB(27, 42, 53)
kachini1.BorderSizePixel = 0
kachini1.Position = UDim2.new(0.19, 0, 0, -27)
kachini1.Size = UDim2.new(0, 300, 0, 26)

kachini2.Name = "GetFFlagShareInviteLinkContextMenuABTestEnabled"
kachini2.CornerRadius = UDim.new(0, 4)
kachini2.Parent = kachini1

kachini3.Name = "GetFStringInExperienceNotificationsLayer"
kachini3.Parent = kachini1
kachini3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
kachini3.BackgroundTransparency = 1.000
kachini3.Position = UDim2.new(0.3, 0, 0.234848887, 0)
kachini3.Size = UDim2.new(0, 124, 0, 15)
kachini3.Font = Enum.Font.GothamSemibold
kachini3.Text = ""
kachini3.TextColor3 = Color3.fromRGB(255,255,255)
kachini3.TextSize = 18.000
kachini3.TextXAlignment = Enum.TextXAlignment.Center

spawn(function()
	while wait() do
		pcall(function()
        	local GameTime = math.floor(workspace.DistributedGameTime+0.5)
        	local Hour = math.floor(GameTime/(60^2))%24
        	local Minute = math.floor(GameTime/(60^1))%60
        	local Second = math.floor(GameTime/(60^0))%60		
			kachini3.Text = "Time : " ..Hour.. " Hr | "..Minute.." Min |  " ..Second.." Sec"
		end)
	end
end)

